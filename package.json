{
  "name": "vs-alias-jump",
  "displayName": "vs-alias-jump",
  "description": "support jump to ailas",
  "version": "1.0.0",
  "publisher": "wudy",
  "engines": {
    "vscode": "^1.18.0"
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/wudys/vs-alias-jump"
  },
  "bugs": {
    "url": "https://github.com/wudys/vs-alias-jump/issues"
  },
  "homepage": "https://github.com/wudys/vs-alias-jump",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "go to definition",
    "alias jump",
    "jump to alias file",
    "jump"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "vs-alias-jump setting",
      "properties": {
        "vs-alias-jump.alias": {
          "type": "object",
          "default": {},
          "scope": "resource",
          "description": "Defines alias for jump to file"
        }
      }
    }
  },
  "devDependencies": {
    "@types/node": "^6.0.40",
    "prettier": "^2.0.5",
    "tslint": "^5.11.0",
    "typescript": "^3.9.7",
    "vscode": "^1.1.17"
  }
}
